{
  "name": "image-charts-website",
  "website": {
    "name": "Image Charts",
    "description": "No more server-side rendering pain, 1 url = 1 chart",
    "title": "Charts, simple as a URL"
  },
  "private": true,
  "version": "5.1.3",
  "scripts": {
    "start": "node static-server",
    "start:dev": "rm -rf dist && parcel serve app/*.pug",
    "build": "npm run --silent build:website",
    "build:update-customers-logo": "node scripts/update-customers-logo.js",
    "build:website": "rm -rf dist && parcel build --detailed-report --no-source-maps --no-cache app/*.pug",
    "printenv": "printenv",
    "release:patch": "npm-release patch",
    "release:minor": "npm-release minor",
    "release:major": "npm-release major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/image-charts/website.git"
  },
  "author": "Kevin Jaffre & FG Ribreau",
  "license": "Image-Charts",
  "homepage": "https://github.com/image-charts/website",
  "devDependencies": {
    "bluebird": "^3.5.3",
    "node-sass": "^4.13.0",
    "npm-release": "^1.0.0",
    "parcel": "^1.12.4",
    "pug": "^2.0.3",
    "sass": "^1.24.4"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^16.3.5",
    "jquery": "^3.1.0",
    "jquery-smooth-scroll": "^1.7.2",
    "jquery-ui": "^1.12.1",
    "lodash.throttle": "^4.1.0",
    "request-promise": "^4.2.2"
  }
}
